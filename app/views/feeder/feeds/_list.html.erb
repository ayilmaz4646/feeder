<div class="blog-posts">
	<section class="timeline">
		<div class="timeline-body">
	  	<%= render "feeder/feeds/box", feeds: feeds %>
	  </div>

	  <% if feeds.respond_to? :current_page %>
		  <button type="button" class="btn btn-default btn-lg btn-block"><%= link_to_next_page feeds, 'Load More', remote: true, id: "load_more_link" %></button>
		<% end %>

	</section>
</div>