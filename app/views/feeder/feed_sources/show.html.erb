<p>
  <strong>Title:</strong>
  <%= @feed_source.title %>
</p>

<p>
  <strong>Url:</strong>
  <%= @feed_source.url %>

  <% if current_user %>
  	<% button_text = 'Follow' %>
  	<%= link to button_text, follow_feed_source(@feed_source), remote: true, id: "feed_source_id", "data-posref" => @user_source %>
  <%end%>
</p>

<%= link_to 'Edit', edit_feed_source_path(@feed_source) %> |
<%= link_to 'Back', feed_sources_path %>
<ul>
	<%= render @feed_source.feeds %>
</ul>
<p>
       <%#= @feeds.entries %>
</p>