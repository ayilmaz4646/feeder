<p>
  <strong>Title:</strong>
  <%= @feed_source.title %>
</p>

<p>
  <strong>Url:</strong>
  <%= @feed_source.url %>

  <% if current_user %>
  	<% if @feed_source.followed_by?(current_user.id) %>
  		<%= link_to 'Unfollow', unfollow_feed_source_path(@feed_source), method: :delete %>
  	<% else %>
  		<%= link_to 'Follow', follow_feed_source_path(@feed_source), remote: true, id: "feed_source_id", "data-posref" => @user_source %>
  	<%end%>
  <%end%>
</p>

<%= link_to 'Edit', edit_feed_source_path(@feed_source) %> |
<%= link_to 'Back', feed_sources_path %>
<ul>
	<%= render @feed_source.feeds %>
</ul>
<p>
       <%#= @feeds.entries %>
</p>
